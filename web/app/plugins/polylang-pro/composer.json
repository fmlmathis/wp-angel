{
  "name": "wpsyntex/polylang-pro",
  "description": "Adds multilingual capability to WordPress (Pro version)",
  "type": "wordpress-plugin",
  "license": "GPL-3.0-or-later",
  "repositories": [
    {
      "type": "path",
      "url": "../polylang",
      "options": {
        "symlink": false
      }
    }
  ],
  "require": {
    "php": ">=7.0",
    "wpsyntex/polylang": "*"
  },
  "autoload": {
    "psr-4": {
      "Polylang\\": "."
    }
  },
  "config": {
    "optimize-autoloader": true,
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
